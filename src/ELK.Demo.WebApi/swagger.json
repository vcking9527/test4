  "paths": {
    "/api/account": {
      "get": {
        "tags": [
          "Account"
        ],
        "summary": "Query",
        "parameters": [
          {
            "name": "PageNumber",
            "in": "query",
            "description": "目前頁面，預設為1",
            "schema": {
              "type": "integer",
              "format": "int32",
              "example": 1
            },
            "example": 1
          },
          {
            "name": "PageSize",
            "in": "query",
            "description": "一個頁面有幾筆，預設為10筆，最大1000筆",
            "schema": {
              "type": "integer",
              "format": "int32",
              "example": 5
            },
            "example": 5
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AccountDtoPaginationListApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/account/all": {
      "get": {
        "tags": [
          "Account"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
