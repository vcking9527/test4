          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "balance": {
            "type": "integer",
            "format": "int32"
          },
          "age": {
            "type": "integer",
            "format": "int32"
          },
          "gender": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AccountDtoPaginationList": {
        "type": "object",
        "properties": {
          "currentPage": {
            "type": "integer",
            "description": "目前頁面的編號",
            "format": "int32"
          },
          "pageSize": {
            "type": "integer",
            "description": "每個頁面的大小",
            "format": "int32"
          },
          "totalPages": {
            "type": "integer",
            "description": "頁面總數量",
            "format": "int32",
            "readOnly": true
          },
          "totalItems": {
            "type": "integer",
            "description": "項目的總數量",
            "format": "int64",
            "readOnly": true
          },
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AccountDto"
            },
            "description": "目前分頁面上的項目",
            "nullable": true
          },
          "hasPreviousPage": {
            "type": "boolean",
            "description": "是否有上一頁",
            "readOnly": true
          },
          "hasNextPage": {
            "type": "boolean",
            "description": "是否有下一頁",
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "AccountDtoPaginationListApiResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "nullable": true
          },
          "data": {
